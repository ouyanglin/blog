Blog::Application.routes.draw do
  get "posts/create"

  get "posts/destroy"

  get "posts/show"

  get "posts/edit"

  resources :sessions, :only => [:new, :create, :destroy]
  resources :users

  match '/signin'  => 'sessions#new'
  match '/signout' => 'sessions#destroy'

  match '/about' => 'pages#about'
  match '/dashboard' => 'pages#dashboard'

  root :to => "pages#home"
end
